<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	
	<managed-bean>
		<managed-bean-name>incluirClientesMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarCliente</managed-bean-class>
		  <managed-bean-scope>request</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listarClientesMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarCliente</managed-bean-class>
		  <managed-bean-scope>session</managed-bean-scope>	
	</managed-bean>	
	<managed-bean>
		<managed-bean-name>incluirInvestidorMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarInvestidor</managed-bean-class>
		  <managed-bean-scope>request</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listarInvestidorMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarInvestidor</managed-bean-class>
		  <managed-bean-scope>request</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>incluirAdministradorMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarAdministrador</managed-bean-class>
		  <managed-bean-scope>request</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listarAdministradorMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarAdministrador</managed-bean-class>
		  <managed-bean-scope>session</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>incluirNoticiasMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarNoticias</managed-bean-class>
		  <managed-bean-scope>request</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listarNoticiasMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarNoticias</managed-bean-class>
		  <managed-bean-scope>session</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>incluirInvestimentoMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarInvestimento</managed-bean-class>
		  <managed-bean-scope>request</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listarInvestimentoMB</managed-bean-name>
		  <managed-bean-class>br.com.jangada.managedbean.CadastrarInvestimento</managed-bean-class>
		  <managed-bean-scope>session</managed-bean-scope>	
	</managed-bean>
	<application>
		<message-bundle>resources.application</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
	</application>
</faces-config>
